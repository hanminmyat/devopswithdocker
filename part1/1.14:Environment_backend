FROM golang:latest

#set env
ENV REQUEST_ORIGIN=http://localhost:5000 

#change Work Dir
WORKDIR /go/src

#expose port
EXPOSE 8080

#Upload files
COPY . .

# Download Dependencies and build
RUN go mod tidy >/dev/null 2>&1 && \
    go build 

# Run go backend
CMD ["/go/src/server"]

